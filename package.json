{
  "name": "anotherkrunkerclient",
  "productName": "anotherkrunkerclient",
  "version": "0.1.0",
  "description": "The be-all and end-all of Krunker clients",
  "author": {
    "name": "Asger Finding",
    "email": "asger.finding@gmail.com"
  },
  "license": "GPLV3",
  "private": true,
  "repository": "asger-finding/anotherkrunkerclient",
  "main": "./build/main.js",
  "scripts": {
    "start": "gulp --state=development && electron ./build/main.js",
    "bundle": "electron-builder",
    "prebundle": "gulp --state=production",
    "rebuild": "electron-builder install-app-deps",
    "postinstall": "yarn rebuild",
    "lint": "yarn eslint . --ext .ts,.js"
  },
  "_moduleAliases": {
    "@color-utils": "src/color-utils",
    "@logger": "src/logger",
    "@constants": "src/config/constants",
    "@client": "src/config/client",
    "@splash-pre-utils": "src/window/splash-pre-utils",
    "@game-api": "src/window/game-api",
    "@game-settings": "src/window/game-settings",
    "@function-hooker": "src/window/function-hooker",
    "@splash-utils": "src/main/splash-utils",
    "@game-utils": "src/main/game-utils",
    "@window-utils": "src/main/window-utils",
    "@event-handler": "src/main/event-handler",
    "@resource-swapper": "src/main/resource-swapper",
    "@useragent-spoof": "src/main/useragent-spoof"
  },
  "dependencies": {
    "@cliqz/adblocker-electron": "^1.23.8",
    "electron-localshortcut": "^3.2.1",
    "electron-store": "^8.0.1",
    "electron-updater": "5.0.5",
    "node-fetch": "^2",
    "semver": "^7.3.6",
    "top-user-agents": "^1.0.46"
  },
  "devDependencies": {
    "@swc/core": "^1.2.212",
    "@types/electron-localshortcut": "^3.1.0",
    "@types/module-alias": "^2.0.1",
    "@types/node": "12.12.21",
    "@types/node-fetch": "^2.6.2",
    "@typescript-eslint/eslint-plugin": "^5.30.6",
    "@typescript-eslint/parser": "^5.30.6",
    "acorn": "^8.7.1",
    "acorn-walk": "^8.2.0",
    "del": "^6.0.0",
    "electron": "^9",
    "electron-builder": "^23.1.0",
    "eslint": "^8.19.0",
    "eslint-config-airbnb-typescript": "^17.0.0",
    "eslint-config-raven": "^1.0.3",
    "eslint-plugin-import": "^2.25.4",
    "event-stream": "^4.0.1",
    "gulp": "^4.0.2",
    "gulp-htmlmin": "^5.0.1",
    "gulp-if": "^3.0.0",
    "gulp-ignore": "^3.0.0",
    "gulp-imagemin": "^7",
    "gulp-sass": "^5.1.0",
    "gulp-swc": "^1.2.3",
    "postcss": "^8.4.12",
    "sass": "^1.53.0",
    "typescript": "^4.7.4",
    "yargs": "^17.4.0"
  }
